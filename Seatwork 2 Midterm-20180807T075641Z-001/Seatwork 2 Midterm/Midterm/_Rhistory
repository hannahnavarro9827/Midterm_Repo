subset <- function(dotosot, min_dotosot,max_dotosot){
x <- ifelse(dotosot$Ozone>min_dotosot & dotosot$Temp>max_dotosot,dotosot$Wind,NA)
mean(x,na.rm = TRUE)
}
subset(dotosot,25,27)
subset <- function(dotosot, min_dotosot,max_dotosot){
x <- ifelse(dotosot$Month=min_dotosot & dotosot$Day=max_dotosot,dotosot$Temp,NA)
mean(x,na.rm = TRUE)
}
subset(dotosot,9,8)
