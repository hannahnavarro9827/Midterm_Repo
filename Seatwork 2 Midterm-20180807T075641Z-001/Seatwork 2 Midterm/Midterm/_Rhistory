MeanFunction <- function(dotosot, Month, Day){
Temp <- 0
for ( row in 1:nrow(dotosot)) {
Temp[row] <- ifelse(dotosot[row,5]==Month&dotosot[row,6]==Day,dotosot[row,4],NA)
}
mean(Temp,na.rm = TRUE)
}
